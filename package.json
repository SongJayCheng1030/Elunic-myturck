    "audit:all": "sh _dev/npm-scripts/npm-audit.sh",
    "audit:all:prod": "sh _dev/npm-scripts/npm-audit-production.sh",
    "shell": "docker-compose -f docker-compose.shell.yml build && docker-compose -f docker-compose.shell.yml -p shopfloorio-turck run --rm --name shopfloor.io-turck-dev --service-ports app bash || true && echo Stopping environment... && docker-compose -f docker-compose.shell.yml -p shopfloorio-turck down",
    "shell:build": "docker-compose -f docker-compose.shell.yml -p shopfloorio-turck build --no-cache",
    "shell:join": "docker exec -it shopfloor.io-turck-dev bash",
    "shell:stop": "docker-compose -f docker-compose.shell.yml -p shopfloorio-turck down",
    "ia": "npm-run-all -l -p -c --aggregate-output i:*",
    "i:shared": "npm i",
    "i:asset-manager-backend": "cd asset-manager-backend/ && npm i",
    "i:condition-monitoring-backend": "cd condition-monitoring-backend/ && npm i",
    "i:data-backend": "cd data-backend/ && npm i",
    "i:file-backend": "cd file-backend/ && npm i",
    "i:hub-backend": "cd hub-backend/ && npm i",
    "i:maintenance-backend": "cd maintenance-backend/ && npm i",
    "i:identity-backend": "cd identity-backend/ && npm i",
    "i:shopfloorio-frontends": "cd shopfloorio-frontends/ && npm i",
    "dev": "concurrently -n hub-server,maintenance-server,user-server,file-backend,asset-backend,condition-monitoring-backend,data-backend \"cd hub-backend && npm run dev\" \"cd maintenance-backend && npm run dev\" \"cd identity-backend && npm run dev\" \"cd file-backend && npm run dev\" \"cd asset-manager-backend && npm run dev\" \"cd condition-monitoring-backend && npm run dev\" \"cd data-backend && npm run dev\"",
    "dev-all": "npm-run-all -l -p -c --aggregate-output dev:*",
    "dev:asset-manager-backend": "cd asset-manager-backend/ && npm run dev",
    "dev:file-backend": "cd file-backend/ && npm run dev",
    "dev:hub-backend": "cd hub-backend/ && npm  run dev",
    "dev:maintenance-backend": "cd maintenance-backend/ && npm  run dev",
    "dev:identity-backend": "cd identity-backend/ && npm run dev",
    "check": "npm-run-all -l -p -c --aggregate-output check:*",
    "check:shared": "eslint -c shared/.eslintrc.check.json --no-eslintrc 'shared/**/*.{ts,js}' && prettier --check 'shared/**/*.{ts,js,html,css,scss,sass,less}'",
    "check:asset-manager-backend": "cd asset-manager-backend/ && npm run check",
    "check:file-backend": "cd file-backend/ && npm run check",
    "check:hub-backend": "cd hub-backend/ && npm run check",
    "check:maintenance-backend": "cd maintenance-backend/ && npm run check",
    "check:identity-backend": "cd identity-backend/ && npm run check",
    "fix": "npm-run-all -l -p -c --aggregate-output fix:*",
    "fix:shared": "eslint -c shared/.eslintrc.fix.json --no-eslintrc --fix 'shared/**/*.{ts,js}' && prettier --write 'shared/**/*.{ts,js,html,css,scss,sass,less}'",
    "fix:asset-manager-backend": "cd asset-manager-backend/ && npm run fix",
    "fix:file-backend": "cd file-backend/ && npm run fix",
    "fix:hub-backend": "cd hub-backend/ && npm run fix",
    "fix:maintenance-backend": "cd maintenance-backend/ && npm run fix",
    "fix:identity-backend": "cd identity-backend/ && npm run fix",
    "precommit": "npx npm-run-all -l -s -c precommit:*",
    "precommit:shared": "lint-staged",
    "//precommit:asset-manager-backend": "cd asset-manager-backend/ && npm run precommit",
    "precommit:condition-monitoring-backend": "cd condition-monitoring-backend/ && npm run precommit",
    "precommit:file-backend": "cd file-backend/ && npm run precommit",
    "precommit:hub-backend": "cd hub-backend/ && npm run precommit",
    "precommit:maintenance-backend": "cd maintenance-backend/ && npm run precommit",
    "//precommit:identity-backend": "cd identity-backend/ && npm run precommit",
    "generate:sio-port-config": "node _fixtures/scripts/generate-sio-portconfig.js"
  },
